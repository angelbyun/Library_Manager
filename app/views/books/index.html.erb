<h1>Books Index Page</h1>
<br>
<% @books.each do |book| %>
  <h2><p>Title: <%= book.title %></p></h2>
  <p>Author: <%= book.author %></p>
  <p>Fiction: <%= book.fiction %></p>
  <p>Number of Copies: <%= book.number_of_copies %></p>
  <p><%= link_to "Edit #{book.title}", "/books/#{book.id}/edit" %></p>
  <p><%= button_to "Delete #{book.title}", "/books/#{book.id}", method: :delete %></p>
  <% end %>